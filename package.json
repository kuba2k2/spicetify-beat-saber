{
  "dependencies": {
    "beatsaver-api": "^2.0.2",
    "jquery": "^3.6.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "scripts": {
    "js": "node scripts/build.js",
    "js:watch": "node scripts/watch.js",
    "css": "npx sass --no-source-map src/scss/style.scss apps/beatsaber/css/beatsaber.css",
    "css:dev": "npx sass src/scss/style.scss apps/beatsaber/css/beatsaber.css",
    "css:watch": "npm run css:dev && npx sass -w src/scss/style.scss apps/beatsaber/css/beatsaber.css",
    "build": "npm run js && npm run css",
    "watch": "npx npm-run-all --parallel js:watch css:watch",
    "start": "npm run watch"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@jsdevtools/sourcemapify": "^2.0.2",
    "@types/jquery": "^3.5.8",
    "@types/react": "^17.0.36",
    "@types/react-dom": "^17.0.11",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.43.5",
    "tinyify": "^3.0.0",
    "tsify": "^5.0.4",
    "watchify": "^4.0.0"
  }
}
